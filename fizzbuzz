def fizzbuzz_convert(number):
    if number % 3 == 0:
        return 'Fizz'


def fizzbuzz_convert(number):
    if number % 5 == 0:
        return 'buzz'

    def fizzbuzz_convert(number):
        if number % 15 == 0:
            return 'Fizzbuzz'

assert fizzbuzz_convert(3) == 'Fizz'
assert fizzbuzz_convert(5) == 'buzz'
assert fizzbuzz_convert(15) == 'Fizzbuzz'

return str(number)
